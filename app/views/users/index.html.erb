<p id="notice"><%= notice %></p>

<h1>Users</h1>

  <div class="list-group">
    <% @users.each do |user| %>
        <%= link_to image_tag(user.photousr.thumb.url.to_s) + " " + user.lastname + ", " + user.firstname + " - " + user.email, user_path(user), class: "list-group-item list-group-item-action" %>
    <% end %>
  </div>
<br>

<%= link_to 'New User', new_user_path, class: "btn btn-success" %>
